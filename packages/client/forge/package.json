{
  "name": "@collaboratory/craft-forge",
  "description": "Client build tool",
  "authors": "Andrew Ewing <drew@collaboratory.io>",
  "version": "0.1.1",
  "main": "dist/main.js",
  "bin": {
    "forge": "bin/forge.js",
    "eslint": "../../../node_modules/eslint/bin/eslint.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-export-extensions",
      "transform-runtime"
    ]
  }
}
