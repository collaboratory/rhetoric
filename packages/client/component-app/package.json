{
  "name": "@collaboratory/craft-client-component-app",
  "description": "The root application container of Craft applications",
  "authors": "Andrew Ewing <drew@collaboratory.io>",
  "version": "0.0.0",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "files": ["dist/", "src/", "README.md"],
  "scripts": {
    "build":
      "babel -d dist/ -D --compact --minified src/ --ignore '**/*.test.js,**/__tests__'",
    "build-forge": "forge component",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "clean": "rm dist/main.js",
    "build-clean": "npm run lint-fix && npm run clean && npm run build"
  },
  "dependencies": {
    "styled-components": "^2.2.4",
    "styled-reset": "^0.1.3",
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  }
}
